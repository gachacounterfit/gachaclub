		public function presetupdatex() : void {
			if(parent.presetclub == 40 && this.openaboutpreset == 1) {
				this.aboutpick = presetchoice + (parent.presetpage * 10 - 10);
				this.aboutpreset.gotoAndStop(2);
				this.openaboutpreset = 2;
			} else {
				parent.colchoice = this.colorchoice;
				this.addpresetnum = parent.presetpage * 10 - 10;
				if(this.swapchoice == 1) {
					parent.importchoice = 1;
					parent.presetpick = presetchoice + this.addpresetnum;
					parent.updatepreset();
				} else if(this.swapchoice == 2) {
					parent.importchoice = 2;
					parent.presetpick = presetchoice + this.addpresetnum;
					parent.updatepreset();
				} else if(this.swapchoice == 3) {
					parent.importchoice = 3;
					parent.presetpick = presetchoice + this.addpresetnum;
					parent.updatepreset();
				} else if(this.swapchoice == 4) {
					parent.importchoice = 4;
					parent.presetpick = presetchoice + this.addpresetnum;
					parent.updatepreset();
				}
				this.openaboutpreset = 1;
				parent.botnav.undobt.visible = true;
			}
		}
		
